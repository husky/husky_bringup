<?xml version="1.0"?>
<package>
  <name>husky_bringup</name>
  <version>0.0.6</version>
  <description>Installation for Husky</description>

  <maintainer email="mpurvis@clearpathrobotics.com">Mike Purvis</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <!-- For roslaunch_add_file_check() macro -->
  <build_depend>roslaunch</build_depend>

  <run_depend>capabilities</run_depend>
  <run_depend>husky_base</run_depend>
  <run_depend>husky_description</run_depend>
  <run_depend>husky_teleop</run_depend>
  <run_depend>imu_compass</run_depend>
  <run_depend>nmea_comms</run_depend>
  <run_depend>nmea_navsat_driver</run_depend>
  <run_depend>robot_pose_ekf</run_depend>
  <run_depend>robot_upstart</run_depend>
  <run_depend>lms1xx</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>um6</run_depend>

  <export>
    <capability_interface>interfaces/IMU.yaml</capability_interface>
    <capability_interface>interfaces/NavSat.yaml</capability_interface>

    <capability_provider>providers/um6.yaml</capability_provider>
    <capability_provider>providers/microstrain.yaml</capability_provider>
    <capability_provider>providers/nmea_navsat.yaml</capability_provider>
  </export>
</package>
